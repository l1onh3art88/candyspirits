// Place all the styles related to the events controller here.
// They will automatically be included in application.css.
// You can use Sass (SCSS) here: http://sass-lang.com/
#eventsbackground{
  border-bottom:.9em solid rgba(255,255,255,0);
}
#events{
  background-image: url(<%= asset_data_uri "News_Events_Gallery background img.png" %>);
  background-size:100%;
  background-repeat:no-repeat;
  background-position: 0% 30%;
  height:30em;
}
#eventstab{
  background-color:white;
  width:43em;
  margin:auto;
  margin-left:35%;
}
#eventsnewspage{
  padding-left:2em;
  padding-right:2em;
  overflow-y:scroll;
  height:27.5em;
  margin-top:-.5em;
}
#eventsnewspage p{
  line-height:1;
}
#eventsnews{
  width: 50%;
  padding: 0px;
  border: 0px;
  z-index: 1000;
  height: 3em;
  background-color: rgba(249, 28, 133, 1);
}
#eventsgallery{
  width: 48.5%;
  padding: 0px;
  border: 0px;
  height: 3em;
}